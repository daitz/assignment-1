import 'dart:io';

void main() {
  stdout.write("10: ");
  var number = int.parse(_readLine());

  if (number > 0) {
    print("Number is positive.");
  } else if (number < 0) {
    print("Number is negative.");
  } else {
    print("Number is neither positive nor negative.");
  }
}

String _readLine() {
  return stdin.readLineSync()!;
}
